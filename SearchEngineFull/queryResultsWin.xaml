<Window x:Class="SearchEngine.queryResultsWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchEngine"
        mc:Ignorable="d"
        Title="queryResultsWin" Height="526" Width="323.333">
    <Grid>
        <Label x:Name="resLbl" Content="The Results:" Visibility="visible" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Width="235" FontFamily="Segoe UI Black" FontSize="30" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="5.629"/>
                    <RotateTransform/>
                    <TranslateTransform X="2.464"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <ListBox x:Name="listBox" HorizontalAlignment="Left"   Height="405" Margin="20,70,0,0" VerticalAlignment="Top" Width="270"  ScrollViewer.VerticalScrollBarVisibility="Visible"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="30" Margin="135,260,0,0" VerticalAlignment="Top" Width="25"/>
    </Grid>
</Window>
